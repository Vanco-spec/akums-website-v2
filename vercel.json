{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },

    { 
      "src": "/", 
    "dest": "/events.html"
   },

    { 
      "src": "/", 
    "dest": "/resources.html"
   },

    { 
      "src": "/", 
    "dest": "/alumni.html"
   },

    { 
      "src": "/", 
    "dest": "/about_us.html"
   },

    { 
      "src": "/", 
    "dest": "/404.html"
   },

    { 
      "src": "/", 
    "dest": "/admin-dashboard.html"
   },

    { 
      "src": "/", 
    "dest": "/constitution.html" 
  },

    {
       "src": "/", 
    "dest": "/contact.html" 
  },

    { 
      "src": "/", 
    "dest": "/leaders.html" 
  },

    { 
      "src": "/", 
    "dest": "/login.html"
   },

    { 
      "src": "/", 
    "dest": "/scorp.html"
   },

    { 
      "src": "/", 
    "dest": "/signup.html"
   },

    { 
      "src": "/", 
    "dest": "/sponsor-dashboard.html" 
  },

    { 
      "src": "/", 
    "dest": "/student-dashboard.html"
   },

    { 
      "src": "/", 
    "dest": "/visitor-dashboard.html" 
  },

    { 
      "src": "/assets/(.*)", 
    "dest": "/assets/$1"
   },

    { "src": "/public/(.*)", 
    "dest": "/public/$1" },

    { "src": "/src/(.*)", 
    "dest": "/src/$1" },

    { "src": "/.*", 
    "dest": "/404.html" }
  ]
}
